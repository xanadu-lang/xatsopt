######
#
# For testing
# the files in xatslib
#
######

######
XATSOPT=\
./../../bin/xatsopt
######
PRELUDE=./../../prelude
XATSLIB=./../../xatslib
######

######

all:: all_libcats
all:: all_githwxi

######
all_libcats:: ; \
$(XATSOPT) -s $(XATSLIB)/libcats/SATS/libcats.sats
######
all_libcats:: ; \
$(XATSOPT) -d $(XATSLIB)/libcats/DATS/g00iout.dats
all_libcats:: ; \
$(XATSOPT) -d $(XATSLIB)/libcats/DATS/stdiout.dats
all_libcats:: ; \
$(XATSOPT) -d $(XATSLIB)/libcats/DATS/fpath00.dats
all_libcats:: ; \
$(XATSOPT) -d $(XATSLIB)/libcats/DATS/rand000.dats
######
all_githwxi:: ; \
$(XATSOPT) -s $(XATSLIB)/githwxi/SATS/githwxi.sats
######
all_githwxi:: ; \
$(XATSOPT) -d $(XATSLIB)/githwxi/DATS/mygist0.dats
all_githwxi:: ; \
$(XATSOPT) -d $(XATSLIB)/githwxi/DATS/mytest0.dats
######
all_githwxi:: ; \
$(XATSOPT) -d $(XATSLIB)/githwxi/DATS/mytree0.dats
all_githwxi:: ; \
$(XATSOPT) -d $(XATSLIB)/githwxi/DATS/dvdcnq0.dats
all_githwxi:: ; \
$(XATSOPT) -d $(XATSLIB)/githwxi/DATS/parcmb1.dats
######

###### end of [Makefile_xatslib] ######
